ifeq ($(LINK),dynamic)
install_orxDLL: 
	$(INSTALL) -d $(prefix)/lib
	$(INSTALL) -m 644 lib/liborx$(orxSUFFIX).so $(prefix)/lib
	install -c lib/liborx$(orxSUFFIX).so $(prefix)/lib
endif

